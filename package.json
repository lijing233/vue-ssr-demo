{
  "name": "vue-ssr-demo",
  "version": "1.0.0",
  "description": "Vue-SSR",
  "main": "index.js",
  "scripts": {
    "dev": "node server/index.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "webpack --mode=production --config build/webpack.client.config.js --progress --hide-modules",
    "build:server": "webpack --mode=production --config build/webpack.server.config.js --progress --hide-modules"
  },
  "author": "lijing <lijing7@gomefinance.com.cn>",
  "license": "ISC",
  "devDependencies": {
    "webpack": "^4.35.2",
    "webpack-cli": "^3.3.5"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "koa": "^2.7.0",
    "vue": "^2.6.10",
    "vue-router": "^3.0.7",
    "vue-server-renderer": "^2.6.10",
    "vuex": "^3.1.1",
    "vuex-router-sync": "^5.0.0"
  }
}
