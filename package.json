  "engines": {
    "node": ">=7.0",
    "npm": ">=4.0"
  },
  "devDependencies": {
    "@babel/core": "^7.5.0",
    "@babel/preset-env": "^7.5.2",
    "babel-loader": "^8.0.6",
    "babel-plugin-dynamic-import-webpack": "^1.1.0",
    "cross-env": "^5.2.0",
    "css-loader": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "mini-css-extract-plugin": "^0.7.0",
    "node-sass": "^4.12.0",
    "sass-loader": "^7.1.0",
    "url-loader": "^2.0.1",
    "vue-loader": "^15.7.0",
    "vue-style-loader": "^4.1.2",
