{
  "name": "vue-ssr-demo",
  "version": "1.0.0",
  "description": "Vue-SSR",
  "main": "index.js",
  "scripts": {
    "dev": "node server/index.js",
    "start": "cross-env NODE_ENV=production node server/index.js",
    "build": "rimraf dist && npm run build:client && npm run build:server",
